{
  "node_name": "${SERVICE_NAME}",
  "server": false,
  "datacenter": "deflectionLB",
  "data_dir": "consul/data",
  "log_level": "DEBUG",
  "retry_join": ["${SERVER_IP}"],
  "service": {
    "id": "${SERVICE_NAME}",
    "name": "${SERVICE_NAME}",
    "tags": ["primary"],
    "address": "localhost",
    "port": 8600,
    "check": {
      "id": "dns",
      "name": "Consul DNS TCP on port 8600",
      "tcp": "localhost:8600",
      "interval": "10s",
      "timeout": "1s"
    }
  },
  "watches" : [
    {
      "type" : "services",
      "args" : ["/users/anakin/ebpf_service_mesh/user_proxy/inform.sh"]
    },
    {
      "type" : "keyprefix",
      "prefix" : "metric/",
      "args" : ["/users/anakin/ebpf_service_mesh/user_proxy/metric_update.py"]
    }
  ]
}
